default partial xkb_types "hypercaps" {
    type "HYPERCAPS" {
        modifiers= Shift+Lock;
        map[Shift] = Level2;
        map[Lock] = Level3;
        map[Shift+Lock] = Level3;
        level_name[Level1]= "Base";
        level_name[Level2]= "Shift";
        level_name[Level3]= "Lock";
    };
};
